generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Athlete {
  id        String    @id @default(cuid())
  name      String
  position  String?
  number    Int?
  lastPlay  DateTime?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}
